{
  "name": "google-search-maps-button",
  "description": "A browser extension that adds a Google Maps button to Google Search results.",
  "private": true,
  "version": "1.0.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appsyde/google-search-maps-button.git"
  },
  "author": {
    "name": "Appsyde",
    "url": "https://appsyde.icu"
  },
  "bugs": {
    "url": "https://github.com/appsyde/google-search-maps-button/issues"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "release-it": "release-it"
  },
  "devDependencies": {
    "release-it": "^19.0.6",
    "typescript": "^5.9.2",
    "wxt": "^0.20.6"
  },
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">=24"
  }
}
